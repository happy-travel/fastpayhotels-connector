{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Authority": {
    "Options": "fastpayhotels-connector/authority"
  },
  "Vault": {
    "Endpoint": "HTDC_VAULT_ENDPOINT",
    "Engine": "secrets",
    "Role": "fastpayhotels",
    "Token": "HTDC_VAULT_TOKEN"
  },
  "Jaeger": {
    "AgentHost": "JAEGER_AGENT_HOST",
    "AgentPort": "JAEGER_AGENT_PORT"
  },
  "Database": {
    "ConnectionString": "Server={0};Port={1};Database=fastpayhotels;Userid={2};Password={3};",
    "Options": "fastpayhotels-connector/connection-string"
  },
  "Fukuoka": {
    "Options": "fukuoka/options"
  },
  "Redis": {
    "Endpoint": "HTDC_REDIS_HOST"
  },
  "RequestLoggingOptions": {
    "IsEnabled": false
  },
  "Telemetry": {
    "IsEnabled": false
  }
}